<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuc.dao.UserMapper">

    <insert id="insert" parameterType="User" keyProperty="id">
        insert into t_user(
        id,
        name)
        values
        (
        #{id},
        #{name})
    </insert>

</mapper>